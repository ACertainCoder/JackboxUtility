{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id":"packs/packPatch.json",
    "title": "PackPatch",
    "type": "object",
    "properties": {
        "id": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "small_description": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "patch_path": {
            "type": "string"
        },
        "configuration": {
            "$ref": "packPatchConfiguration.json"
        },
        "components": {
            "type": "array",
            "items": {
                "$ref": "packPatchComponent.json"
            }
        }
    },
    "required": [
        "id",
        "name",
        "small_description",
        "description",
        "patch_path",
        "configuration",
        "components"
    ]
    
}